<ejs-sidebar id="sidebar" width="290px" enableDock dockSize="100px" target=".dock-target" enableGestures="false">
    <e-content-template>

        <div class="logo-wrapper">
            <div class="app-logo">
                <img src="~/logo.png" />
            </div>
            <div class="w-100"></div>
            <i class="fa-solid fa-angles-left" id="sidebar-toggler"></i>
        </div>

        <div class="profile-wrapper">
            <img src="~/profile.png" class="profile-pic" />
            <div class="titles d-flex flex-column ps-3">
                <h6 class="mb-0">Wallet App</h6>
                <span class="text-muted">Iron Man</span>
            </div>
        </div>
        <ejs-menu id="menu" items="menuItems" orientation="Vertical"></ejs-menu>
    </e-content-template>
</ejs-sidebar>

<script>
    document.addEventListener('DOMContentLoaded', function(){
        dockbar = document.getElementById('sidebar').ej2_instances[0];
        document.getElementById('sidebar-toggler').onclick = function(){
            dockbar.toggle();
        }
    })
</script>